{
    "extractor": {
        "base-directory": "media-downloads",
        "archive": null,
		
		"instagram": {
            "directory": ["Instagram", "{post_shortcode}"],
			"videos": "ytdl",
			"ytdl" : {
				"format": "avc1"
			},
			"ytdl-args" : [
				"-f", "bv[ext=mp4][vcodec^=avc]+ba[ext=m4a]/b[ext=mp4]"
			],
            "filename": {
                "count > 1": "{post_shortcode}_{num}.{extension}",
                "": "{post_shortcode}.{extension}"
            }
        }
    }
}